<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="com.glasserino.caller.ui.custom.GlScalableGlCardGlViewGl.ScaleType"/>
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintDimensionRatio="360:772"
            android:clipChildren="false">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintDimensionRatio="310:370"
                app:layout_constraintWidth_percent="0.86111"
                android:clipChildren="false">

                <com.glasserino.caller.ui.custom.GlScalableGlCardGlViewGl
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:cardBackgroundColor="@android:color/transparent"
                    app:cardElevation="0dp"
                    scaleType="@{ScaleType.BY_WIDTH}"
                    scalableCornerRadius="@{0.064516f}">
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="127"
                            android:background="@drawable/blue_gradient"/>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="243"
                            android:background="@drawable/black_gradient">

                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintDimensionRatio="282:22"
                                app:layout_constraintHeight_percent="0.09"
                                app:layout_constraintVertical_bias="0.0969"
                                android:gravity="center"
                                app:autoSizeTextType="uniform"
                                app:autoSizeMaxTextSize="20sp"
                                app:autoSizeMinTextSize="1sp"
                                android:textColor="#FFFFFF"
                                android:fontFamily="@font/roboto_bold"
                                android:text="@string/permissions"/>

                            <androidx.appcompat.widget.AppCompatTextView
                                android:id="@+id/textDescription"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintDimensionRatio="282:32"
                                app:layout_constraintHeight_percent="0.131687"
                                app:layout_constraintVertical_bias="0.32227"
                                android:gravity="center"
                                app:autoSizeTextType="uniform"
                                app:autoSizeMaxTextSize="14sp"
                                app:autoSizeMinTextSize="1sp"
                                android:textColor="#FFFFFF"
                                android:fontFamily="@font/roboto_regular"/>

                            <com.glasserino.caller.ui.custom.GlScalableGlCardGlViewGl
                                android:id="@+id/buttonNo"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintDimensionRatio="88:37"
                                app:layout_constraintHeight_percent="0.15226"
                                app:layout_constraintVertical_bias="0.7087"
                                app:layout_constraintHorizontal_bias="0.22072"
                                app:cardElevation="0dp"
                                app:cardBackgroundColor="@android:color/transparent"
                                scaleType="@{ScaleType.BY_HEIGHT}"
                                scalableCornerRadius="@{0.5f}">
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/button_bg">
                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="0dp"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintDimensionRatio="51:16"
                                        app:layout_constraintHeight_percent="0.432432432"
                                        android:gravity="center"
                                        app:autoSizeTextType="uniform"
                                        app:autoSizeMaxTextSize="14sp"
                                        app:autoSizeMinTextSize="1sp"
                                        android:textColor="#FFFFFF"
                                        android:fontFamily="@font/roboto_bold"
                                        android:text="@string/cancel"
                                        tools:ignore="TooDeepLayout" />
                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </com.glasserino.caller.ui.custom.GlScalableGlCardGlViewGl>

                            <com.glasserino.caller.ui.custom.GlScalableGlCardGlViewGl
                                android:id="@+id/buttonYes"
                                android:layout_width="0dp"
                                android:layout_height="0dp"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintDimensionRatio="75:37"
                                app:layout_constraintHeight_percent="0.15226"
                                app:layout_constraintVertical_bias="0.7087"
                                app:layout_constraintHorizontal_bias="0.7957"
                                app:cardElevation="0dp"
                                app:cardBackgroundColor="@android:color/transparent"
                                scaleType="@{ScaleType.BY_HEIGHT}"
                                scalableCornerRadius="@{0.5f}">
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/button_bg">
                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:layout_width="0dp"
                                        android:layout_height="0dp"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintDimensionRatio="38:16"
                                        app:layout_constraintHeight_percent="0.432432432"
                                        android:gravity="center"
                                        app:autoSizeTextType="uniform"
                                        app:autoSizeMaxTextSize="14sp"
                                        app:autoSizeMinTextSize="1sp"
                                        android:textColor="#FFFFFF"
                                        android:fontFamily="@font/roboto_bold"
                                        android:text="@string/ok" />
                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </com.glasserino.caller.ui.custom.GlScalableGlCardGlViewGl>

                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.appcompat.widget.LinearLayoutCompat>
                </com.glasserino.caller.ui.custom.GlScalableGlCardGlViewGl>

                <View
                    android:id="@+id/markerVertical"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintDimensionRatio="310:127"/>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/markerHorizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.44516"/>

                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="@id/markerVertical"
                    app:layout_constraintStart_toStartOf="@id/markerHorizontal"
                    app:layout_constraintDimensionRatio="188:108"
                    app:layout_constraintWidth_percent="0.60645"
                    android:src="@mipmap/img_permission"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>