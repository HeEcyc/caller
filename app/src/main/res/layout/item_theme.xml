<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="com.galaxy.call.ui.custom.GScalableGCardGViewG.ScaleType"/>
        <import type="android.view.View"/>
        <variable
            name="item"
            type="com.galaxy.call.ui.home.GThemeGAdapterG.ThemeViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.galaxy.call.ui.custom.GScalableGCardGViewG
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="161:277"
            app:cardElevation="0dp"
            app:cardBackgroundColor="@android:color/transparent"
            scaleType="@{ScaleType.BY_WIDTH}"
            scalableCornerRadius="@{0.074534f}">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/preview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.galaxy.call.ui.custom.GScalableGCardGViewG>
    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>